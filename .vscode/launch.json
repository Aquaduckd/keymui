{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug keymui",
            "cargo": {
                "args": [
                    "build",
                    "--bin=keymui",
                    "--package=keymui"
                ],
                "filter": {
                    "name": "keymui",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {},
            "sourceLanguages": ["rust"]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug keymui (Release)",
            "cargo": {
                "args": [
                    "build",
                    "--release",
                    "--bin=keymui",
                    "--package=keymui"
                ],
                "filter": {
                    "name": "keymui",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {},
            "sourceLanguages": ["rust"]
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Debug keymui (GDB)",
            "program": "${workspaceFolder}/target/debug/keymui",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cargo-build-debug"
        }
    ]
}

